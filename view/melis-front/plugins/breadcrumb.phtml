<ol class="breadcrumb">
    <?php foreach ($this->breadcrumb As $key => $val):?>
    	<?php if ($val['isActive']):?>
    		<li class="active"><?php echo $val['label']?></li>
    	<?php else:?>
    		<li><a title="<?php echo $val['label']  ?>" href="<?php echo $val['uri']?>"><?php echo $val['label'] ?></a></li>
    	<?php endif;?>
    <?php endforeach;?>
</ol>